<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netcoreapp3.0</TargetFramework>
  </PropertyGroup>
  <ItemGroup>
    <!--
      To install next 3 references we need to install extension "NuGet Package Manager" 
      in VsCode. Then press Ctrl + Shift + P and select "NuGet Package Manager: Add Package".
      Then look for "Microsoft.NET.Test.Sdk", "xunit" and "xunit.runner.visualstudio" literals.
    -->

    <PackageReference Include="Microsoft.NET.Test.Sdk" Version="16.5.0"/>
    <PackageReference Include="xunit" Version="2.4.1"/>
    <PackageReference Include="xunit.runner.visualstudio" Version="2.4.1"/>
  </ItemGroup>
</Project>